{
	"name": "koreans-in-usa",
	"version": "1.0.0",
	"description": "job searching and information sharing",
	"main": "index.js",
	"scripts": {
		"watch:sass": "node-sass sass/main.scss css/style.css -w",
		"devserver": "live-server --browser=firefox",
		"start": "npm-run-all --parallel devserver watch:sass",
		"compile:sass": "node-sass sass/main.scss css/style.comp.css",
		"prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
		"compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
		"build:css": "npm-run-all compile:sass prefix:css compress:css"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DavidLee8two7/Koreans-in-USA.git"
	},
	"author": "David Lee",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/DavidLee8two7/Koreans-in-USA/issues"
	},
	"homepage": "https://github.com/DavidLee8two7/Koreans-in-USA#readme",
	"devDependencies": {
		"autoprefixer": "^9.1.2",
		"concat": "^1.0.3",
		"live-server": "^1.2.0",
		"node-sass": "^4.9.3",
		"npm-run-all": "^4.1.3",
		"postcss-cli": "^6.0.0"
	}
}
